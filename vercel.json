{
  "version": 2,
  "buildCommand": "cd hackhive-platform && npm install && npm run build",
  "outputDirectory": "hackhive-platform/.next",
  "installCommand": "cd hackhive-platform && npm install",
  "framework": "nextjs",
  "builds": [
    {
      "src": "hackhive-platform/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "hackhive-platform/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_MOCK_MODE": "true"
  }
}